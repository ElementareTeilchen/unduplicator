{
    "name": "elementareteilchen/unduplicator",
    "type": "typo3-cms-extension",
    "description": "Finds duplicates in sys_file and unduplicates them",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4 || ^8.5",
        "ext-pdo": "*",
        "typo3/cms-core": "^12.4 || ^13.4"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "unduplicator",
            "web-dir": ".Build/Web"
        }
    },
    "autoload": {
        "psr-4": {
            "ElementareTeilchen\\Unduplicator\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElementareTeilchen\\Unduplicator\\Tests\\": "Tests"
        }
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin",
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    },
    "require-dev": {
        "netresearch/rte-ckeditor-image": "^12.0 || ^13.0",
        "typo3/testing-framework": "^8.3 || ^9.3"
    }
}
